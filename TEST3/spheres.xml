<?xml version="1.0" encoding="UTF-8" ?>
<!--  Bunny-mesh (500 polygons)
 -->
<scene>
	<samples>16</samples>
  <camera>
    <viewPoint>2 4.5 8</viewPoint>
    <viewDir>-2 -4 -8</viewDir>
    <projNormal>2 4 8</projNormal>
    <viewUp>0 1 0</viewUp>
    <projDistance>2.5</projDistance>
    <viewWidth>1</viewWidth>
    <viewHeight>0.708571429</viewHeight>
  </camera>
  <image>
    350 248
  </image>
  <shader name="white" type="Glazed">
    <refractiveIndex>20</refractiveIndex>
		<substrate type="Phong">
			<diffuseColor>.99 .99 .99</diffuseColor>
			<specularColor>.99 .99 .99</specularColor>
			<exponent>100</exponent>
		</substrate>
  </shader>
  <shader name="black" type="Glazed">
    <refractiveIndex>20</refractiveIndex>
		<substrate type="Phong">
			<diffuseColor>0 0 0</diffuseColor>
			<specularColor>0 0 0</specularColor>
			<exponent>100</exponent>
		</substrate>
  </shader>
  <shader name="blue" type="Glazed">
    <refractiveIndex>20</refractiveIndex>
		<substrate type="Phong">
			<diffuseColor>0 0 1</diffuseColor>
			<specularColor>0 0 1</specularColor>
			<exponent>100</exponent>
		</substrate>
  </shader>
  <shader name="green" type="Glazed">
    <refractiveIndex>20</refractiveIndex>
		<substrate type="Phong">
			<diffuseColor>0 1 0</diffuseColor>
			<specularColor>0 1 0</specularColor>
			<exponent>100</exponent>
		</substrate>
  </shader>
  <shader name="red" type="Glazed">
    <refractiveIndex>20</refractiveIndex>
		<substrate type="Phong">
			<diffuseColor>1 0 0</diffuseColor>
			<specularColor>1 0 0</specularColor>
			<exponent>100</exponent>
		</substrate>
  </shader>
  <surface type="Box">
  	<minpt>-6 -2.5 -6</minpt>
  	<maxpt>6 1 6</maxpt>
  	<shader ref="white" />
  </surface>
  <!-- New Row -->
  <surface type="Box">
  	<minpt>-2 .5 -3</minpt>
  	<maxpt>-1.2 1.001 -2</maxpt>
  	<shader ref="black" />
  </surface>
  <surface type="Box">
  	<minpt>-.4 .5 -3</minpt>
  	<maxpt>.4 1.001 -2</maxpt>
  	<shader ref="black" />
  </surface>
  <surface type="Box">
  	<minpt>1.2 .5 -3</minpt>
  	<maxpt>2.0 1.001 -2</maxpt>
  	<shader ref="black" />
  </surface>
  <!-- New Row -->
  <surface type="Box">
  	<minpt>-2.8 .5 -2</minpt>
  	<maxpt>-2 1.001 -1</maxpt>
  	<shader ref="black" />
