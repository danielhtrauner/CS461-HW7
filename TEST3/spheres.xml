  	<shader ref="black" />
  </surface>  
  
  
  
  
  
  
  
  
  
  
  <surface type="Sphere">
    <shader ref="red" />
    <center>-.6 1.3 1.1</center>
    <radius>.4</radius>
  </surface> 
  <surface type="Sphere">
    <shader ref="green" />
